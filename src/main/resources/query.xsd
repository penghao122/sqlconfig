<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
xmlns:cdr="http://www.duowan.com" targetNamespace="http://www.duowan.com">
    <xsd:element name="sqlConfig">
        <xsd:complexType>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">

                <xsd:element name="sql-query">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="name" type="xsd:string" />
                        </xsd:sequence>
                        <xsd:attribute name="sql" type="xsd:string" use="required"/>
                    </xsd:complexType>
                </xsd:element>
         
            </xsd:choice>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
